{
  "name": "Telegram",
  "dockerFile": "Dockerfile",
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/vscode/workspace,type=bind,consistency=cached",
  "workspaceFolder": "/home/vscode/workspace/",
  "mounts": [
    "source=telegram-node_modules,target=/home/vscode/workspace/node_modules,type=volume"
  ],
  "postCreateCommand": "sudo chown -hR vscode:vscode ~/workspace",
  "remoteUser": "vscode"
}
