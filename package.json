{
  "name": "@lannuttia/telegram",
  "version": "2.0.1",
  "description": "Generic brokers designed for routing information in a single process",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "npx eslint src/",
    "test": "npx jest --coverage",
    "build": "npx babel src -d lib",
    "doc": "npx jsdoc --pedantic -R ./README.md -P ./package.json -d docs src",
    "preversion": "npm run test",
    "version": "npm run doc && git add -A src/ docs/",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "publish",
    "subscribe",
    "publish/subscribe",
    "pub/sub",
    "request",
    "resposne",
    "request/response",
    "req/resp",
    "decouple",
    "mediator",
    "message broker",
    "broker"
  ],
  "author": "Anthony Lannutti <lannuttia@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lannuttia/telegram.git"
  },
  "dependencies": {},
  "optionalDependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.2"
  }
